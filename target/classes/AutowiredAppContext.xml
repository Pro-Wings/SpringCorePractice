<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config />
    <bean class="autowired.Employee" name="employeeOne" >
<!--
        <property name="id" value="12" />
-->
        <property name="name" value="ABC" />
    </bean>

    <bean class="autowired.EmpAddress" name="xyz">
        <property name="city"><value>"Mumbai"</value></property>
        <property name="country"><value>"India"</value></property>
        <property name="pin"><value>123</value></property>
    </bean>

    <bean class="autowired.EmpAddress" name="abc">
        <property name="city"><value>"Pune"</value></property>
        <property name="country"><value>"India"</value></property>
        <property name="pin"><value>345</value></property>
    </bean>


</beans>